{
  "status": 0,
  "msg": "",
  "data": {
    "pages": [
      {
        "label": "Home",
        "url": "/",
        "redirect": "/settings"
      },
      {
        "label": "",
        "children": [
          {
            "label": "内容管理",
            "url": "/article",
            "icon": "fa fa-file-text",
            "schemaApi": "get:/pages/article-list.json",
            "children": [
              {
                "label": "创建文章",
                "url": "/article/new",
                "icon": "fa fa-plus",
                "schemaApi": "get:/pages/article-new.json"
              },
              {
                "label": "编辑文章",
                "url": "/article-edit/:id",
                "schemaApi": "get:/pages/article-edit.json"
              }
            ]
          },
          {
            "label": "资源管理",
            "url": "/media",
            "icon": "fa fa-file-image-o",
            "schemaApi": "get:/pages/media-list.json"
          },
          {
            "label": "公司品牌",
            "url": "/brands",
            "icon": "fa fa-university",
            "schema": {
              "type": "page",
              "title": "页面C",
              "body": "页面C"
            }
          },
          {
            "label": "反馈管理",
            "url": "/feedback",
            "icon": "fa fa-comment",
            "schema": {
              "type": "page",
              "title": "页面C",
              "body": "页面C"
            }
          },
          {
            "label": "系统设置",
            "url": "/settings",
            "icon": "fa fa-cog",
            "schema": {
              "type": "page",
              "initApi": "${API_HOST}/v1/administrator/secure/me",
              "title": "Dashboard",
              "body": [
                {
                  "type": "divider"
                },
                {
                  "type": "button",
                  "label": "修改密码",
                  "onEvent": {
                    "click": {
                      "actions": []
                    }
                  }
                },
                {
                  "type": "divider"
                },
                {
                  "type": "button",
                  "label": "设置两步验证",
                  "onEvent": {
                    "click": {
                      "actions": [
                        {
                          "ignoreError": false,
                          "outputVar": "result",
                          "actionType": "ajax",
                          "options": {
                            "silent": true
                          },
                          "api": {
                            "url": "${API_HOST}/v1/administrator/secure/me",
                            "method": "get"
                          }
                        },
                        {
                          "ignoreError": false,
                          "actionType": "dialog",
                          "dialog": {
                            "type": "dialog",
                            "title": "设置两步验证",
                            "body": [
                              {
                                "type": "tpl",
                                "tpl": "<p><span class=\"cxd-TplField mb-3\">两步验证可以为您的账户提供额外的安全保护。启用后，每次登录时都需要输入验证码。</span></p>"
                              },
                              {
                                "type": "switch",
                                "label": "当前状态",
                                "name": "switch",
                                "falseValue": false,
                                "trueValue": true,
                                "onText": "已启用",
                                "offText": "未启用",
                                "value": "${IF(two_factor_enabled,true,false)}",
                                "onEvent": {
                                  "change": {
                                    "actions": [
                                      {
                                        "actionType": "ajax",
                                        "api": {
                                          "url": "${API_HOST}/v1/administrator/secure/toggle-2fa",
                                          "method": "post",
                                          "data": {
                                            "enabled": "${event.data.value}"
                                          }
                                        }
                                      }
                                    ]
                                  }
                                },
                                "mode": "horizontal",
                                "optionAtLeft": true
                              }
                            ],
                            "actions": [
                              {
                                "type": "button",
                                "label": "重置密钥",
                                "level": "danger",
                                "onEvent": {
                                  "click": {
                                    "actions": [
                                      {
                                        "ignoreError": false,
                                        "outputVar": "key_2fa",
                                        "actionType": "ajax",
                                        "options": {
                                          "silent": true
                                        },
                                        "api": {
                                          "url": "${API_HOST}/v1/administrator/secure/enable-2fa",
                                          "method": "post",
                                          "requestAdaptor": "",
                                          "adaptor": "",
                                          "messages": {},
                                          "silent": true,
                                          "replaceData": true
                                        }
                                      },
                                      {
                                        "ignoreError": false,
                                        "actionType": "dialog",
                                        "dialog": {
                                          "type": "dialog",
                                          "title": "重置密钥",
                                          "body": [
                                            {
                                              "type": "tpl",
                                              "tpl": "请使用两步验证客户端扫描下方二维码或复制密钥",
                                              "wrapperComponent": "h3"
                                            },
                                            {
                                              "type": "input-text",
                                              "label": "密钥",
                                              "name": "text",
                                              "staticOn": "",
                                              "value": "${secret}"
                                            },
                                            {
                                              "type": "image",
                                              "enlargeAble": false,
                                              "maxScale": 200,
                                              "minScale": 50,
                                              "style": {
                                                "display": "inline-block"
                                              },
                                              "title": "二维码",
                                              "width": 160,
                                              "height": 160,
                                              "imageMode": "thumb",
                                              "src": "${qrcode}"
                                            },
                                            {
                                              "type": "input-text",
                                              "label": "验证码",
                                              "name": "code",
                                              "maxLength": 6,
                                              "required": true,
                                              "validations": {
                                                "maxLength": 6,
                                                "minLength": 6
                                              }
                                            }
                                          ],
                                          "actions": [
                                            {
                                              "type": "button",
                                              "actionType": "confirm",
                                              "label": "提交",
                                              "primary": true,
                                              "onEvent": {
                                                "click": {
                                                  "weight": 0,
                                                  "actions": [
                                                    {
                                                      "ignoreError": false,
                                                      "outputVar": "responseResult",
                                                      "actionType": "ajax",
                                                      "options": {},
                                                      "api": {
                                                        "method": "post",
                                                        "url": "${API_HOST}/v1/administrator/secure/verify-2fa",
                                                        "data": {
                                                          "code": "${code}",
                                                          "secret": "${secret}"
                                                        }
                                                      }
                                                    }
                                                  ]
                                                }
                                              }
                                            }
                                          ],
                                          "showCloseButton": true,
                                          "closeOnOutside": false,
                                          "closeOnEsc": false,
                                          "showErrorMsg": true,
                                          "showLoading": true,
                                          "draggable": false
                                        }
                                      }
                                    ]
                                  }
                                }
                              }
                            ]
                          }
                        }
                      ]
                    }
                  }
                }
              ]
            },
            "children": [
              {
                "label": "多语言支持",
                "url": "/settings/language",
                "icon": "fa fa-language",
                "schemaApi": "get:/pages/language-list.json"
              },
              {
                "label": "管理员",
                "url": "/settings/adminuser",
                "icon": "fa fa-user",
                "schemaApi": "get:/pages/adminuser-list.json"
              },
              {
                "label": "模型管理",
                "url": "/settings/models",
                "icon": "fa fa-cubes",
                "schema": {
                  "type": "page",
                  "title": "页面C",
                  "body": "页面C"
                }
              },
              {
                "label": "模块管理",
                "url": "/settings/mods",
                "icon": "fa fa-th-large",
                "schemaApi": "get:/pages/mod-list.json"
              }
            ]
          },
          {
            "label": "列表示例",
            "url": "/crud",
            "rewrite": "/crud/list",
            "icon": "fa fa-cube",
            "children": [
              {
                "label": "列表",
                "url": "/crud/list",
                "icon": "fa fa-list",
                "schemaApi": "get:/pages/crud-list.json"
              },
              {
                "label": "新增",
                "url": "/crud/new",
                "icon": "fa fa-plus",
                "schemaApi": "get:/pages/crud-new.json"
              },
              {
                "label": "查看",
                "url": "/crud/:id",
                "schemaApi": "get:/pages/crud-view.json"
              },
              {
                "label": "修改",
                "url": "/crud/:id/edit",
                "schemaApi": "get:/pages/crud-edit.json"
              }
            ]
          }
        ]
      }
    ]
  }
}